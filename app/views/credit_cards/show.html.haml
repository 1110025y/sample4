.contents
  = render "header-top"
  .contents__body
    = render "side-bar"
    .show-contents
      %label 登録クレジットカード情報
      %br
      = "**** **** **** " + @default_card_information.last4
      %br
      - exp_month = @default_card_information.exp_month.to_s
      - exp_year = @default_card_information.exp_year.to_s.slice(2,3)
      = exp_month + " / " + exp_year
      = link_to '削除する', credit_card_path, method: :delete, class: "destroy-function-link-btn"
      
  .contents__box
  .footer
  = render "footer-bottom"



